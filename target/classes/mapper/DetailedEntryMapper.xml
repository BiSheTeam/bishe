<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="team.bishe.wms.mapper.DetailedEntryMapper">
    <resultMap id="DetailedEntry" type="team.bishe.wms.bean.DetailedEntry">
        <id property="id" column="id"/>
        <result property="customerId" column="customerId"/>
        <result property="checkerId" column="checkerId"/>
        <result property="storekeeperId" column="storekeeperId"/>
        <result property="goodsName" column="goodsName"/>
        <result property="goodsSize" column="goodsSize"/>
        <result property="repositoryId" column="repository_id"/>

    </resultMap>

    <select id="selectRandom" resultMap="DetailedEntry">
        select *
        from wms
        where 1=1
        <if test="param1 != null and param1 !=''" >and id = #{param1}</if>
        <if test="param2 != null and param2 !=''">and customer = #{param2}</if>
        <if test="param3 != null and param3 !='' and param4 != null and param4 !=''">and time between  #{param3}  and #{param4}</if>
    </select>

    <select id="selectAll" resultMap="DetailedEntry">
        select * from 入库单
    </select>

    <select id="selectById" resultMap="DetailedEntry">

    </select>

    <select id="selectByCusId" resultMap="DetailedEntry">

    </select>

    <select id="selectByTime" resultMap="DetailedEntry">

    </select>

    <insert id="insert">

    </insert>

    <update id="update">

    </update>

    <update id="updateById">
        update wms_detailedEntry
        set repository_id = #{param2}
        where id = #{param1}
    </update>

    <delete id="deleteAll">

    </delete>

    <delete id="deleteById">

    </delete>
</mapper>
