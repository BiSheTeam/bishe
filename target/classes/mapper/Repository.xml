<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="team.bishe.wms.mapper.RepositoryMapper">
    <resultMap id="repository" type="team.bishe.wms.bean.Repository">
        <id property="id" column="REPO_ID"/>
        <result property="address" column="REPO_ADDRESS"/>
        <result property="status" column="REPO_STATUS"/>
        <result property="area" column="REPO_AREA"/>
        <result property="desc" column="REPO_DESC"/>
        <result property="adminId" column="REPO_ADMIN_ID"/>
        <result property="adminName" column="REPO_ADMIN_NAME"/>
    </resultMap>

    <select id="selectAll" resultMap="repository">
        select * from wms_repository
    </select>
    
    <select id="selectByID" resultMap="repository">
        select * from wms_repository where repo_id = #{id}
    </select>
    
    <select id="selectByAddress" resultMap="repository">
        select * from wms_repository where repo_address = #{address}
    </select>

    <select id="selectUnassign" resultMap="repository">

    </select>

    <insert id="insert">
        insert into wms_repository (repo_address,repo_status,repo_area,reps_desc) values (#{address},#{status},#{area},#{desc})
    </insert>

    <insert id="insertbatch">

    </insert>

    <update id="update">
        update wms_repository set repo_address = #{address},repo_status = #{status},repo_area = #{area},repo_desc = #{desc}
        where repo_id = #{id}
    </update>

    <delete id="deleteByID">
        delete from wms_repository where repo_id = #{id}
    </delete>

</mapper>
