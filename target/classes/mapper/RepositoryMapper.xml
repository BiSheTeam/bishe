<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="team.bishe.wms.mapper.RepositoryMapper">

    
    <select id="selectByID" resultType="team.bishe.wms.bean.Repository">
        select whouse_nm as name
        from t_warehouse_info
        where whouse_id = #{id}
    </select>

    <select id="selectByStatus" resultType="team.bishe.wms.bean.Repository">
        select whouse_id as id,
            whouse_nm as name
        from t_warehouse_info
        where status = "kong"
    </select>



</mapper>
